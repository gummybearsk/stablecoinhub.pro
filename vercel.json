{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "stablecoinhub.pro"
        }
      ],
      "destination": "https://www.stablecoinhub.pro/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "blog.stablecoinhub.pro"
        }
      ],
      "destination": "https://www.stablecoinhub.pro/blog/$1",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/:path*/index.html",
      "destination": "/:path*/",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Link",
          "value": "<https://www.stablecoinhub.pro/$1>; rel=\"canonical\""
        }
      ]
    }
  ]
}